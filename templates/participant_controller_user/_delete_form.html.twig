<form method="post" action="{{ path('app_participation_delete', {'idpart': participant.idpart}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ participant.idpart) }}">
    <button class="btn" class="btn btn-secondary" style="background-color: #72ae44; color: white; border-color: #72ae44; border-radius: 30px; width: 150px;">Supprimer</button>
</form>
